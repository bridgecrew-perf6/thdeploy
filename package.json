{
	"name": "thdeploy",
	"displayName": "thdeploy",
	"description": "ssh deploy",
	"publisher": "thgq",
	"icon": "media/thdeploy.png",
	"version": "1.0.7",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:nodeDependencies"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nodeDependencies.refreshEntry",
				"title": "刷新",
				"icon": {
					"light": "resources/refresh_light.svg",
					"dark": "resources/refresh_dark.svg"
				}
			},
			{
				"command": "nodeDependencies.uploadEntry",
				"title": "上传",
				"icon": {
					"light": "resources/upload_light.svg",
					"dark": "resources/upload_dark.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "nodeDependencies.refreshEntry",
					"when": "view == nodeDependencies",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "nodeDependencies.uploadEntry",
					"when": "view == nodeDependencies && viewItem == dependency",
					"group": "inline"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "ssh服务",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "nodeDependencies",
					"name": "ssh服务",
					"icon": "media/dep.svg",
					"contextualTitle": "ssh服务"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^12.11.7",
		"@types/ssh2": "^0.5.51",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"archiver": "^5.0.2",
		"chalk": "^4.1.0",
		"inquirer": "^7.3.3",
		"lru-cache": "^7.7.1",
		"node-ssh": "^11.1.1",
		"ora": "^5.1.0"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/th-come/thdeploy.git/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/th-come/thdeploy.git"
	},
	"homepage": "https://github.com/th-come/thdeploy.git"
}
